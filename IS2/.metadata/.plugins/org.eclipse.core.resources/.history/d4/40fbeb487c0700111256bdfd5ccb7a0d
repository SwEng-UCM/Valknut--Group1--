import combat.Chaos;

public class Main {

	/**
	 * Entry point
	 * 
	 * @param args Arguments for the game.
	 */
	public static void main(String[] args) {
		Chaos enemy_one = new Chaos();
		boolean alive = true;
		
		while(alive) {
			String[] attack = enemy_one.getPrompt();
			alive = enemy_one.receiveAttack(attack[0]);
		}
	}
}
